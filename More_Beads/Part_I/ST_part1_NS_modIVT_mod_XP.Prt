(1-1-1)[ProtocolInfo]
AddToLogFile ">(1-1-1)"
; ST_part1.prt
; AJ
; ST part 1 automated sample prep for MBS 8000
; SOP v.9

; Add protocol info to log file:
AddToLogFile ""
AddToLogFile "SOP v.9"
AddToLogFile ""


; ======================================================INITIALIZATION

(1-2-1)[Initialize]
AddToLogFile ">(1-2-1)"
AddToLogFile ">(1-2-1)"
AddToLogFile "Protocol started"
RunMessage "Initializing protocol..."
; Start protocol timer:
Gval startTime New
Gval startTime Let @SECONDCOUNT
;org all
_SetTemp 1 $EnzymeTemp
_SetTemp 2 $SSSTemp
Call HomeMotors
MoveTo TRASH
ActionZ #Z_SPEED_H
Org P W
SelectTip Standard
SetTrashPosition TRASH
TipOff

(1-3-1)[SetPlateVolumes]
AddToLogFile ">(1-3-1) setting plate volumes"

; Reaction plate
SetPlateVolume SECOND_STRAND_SYNTHESIS 0
SetPlateVolume BLUNTING_REACTION 0
SetPlateVolume XTRAWORKPOS 0
SetPlateVolume IVT 0
SetPlateVolume POST_IVT 0

; Deep well plate
SetPlateVolume XP_BEADS $xpTotalBeadVol
SetPlateVolume ETOH $xpTotalEthanolVol
SetPlateVolume ETOH2 $xpSecondEthanolVol
SetPlateVolume ELUTION_BUFFER $xpTotalElutionVol
SetPlateVolume WATER $TotalDilutionVol
;SetPlateVolume WASH_BUFFER $xpTotalWashVol
SetPlateVolume OIL $TotalOilVol

; Cooling plate
SetPlateVolume SAMPLE $StartVol
SetPlateVolume SS_MIX $SSMixVol
SetPlateVolume BLUNTING_MIX $BluntingMixVol
SetPlateVolume EDTA $EDTAVol
SetPlateVolume IVT_MIX $IVTMixVol
SetPlateVolume DNTP_MIX $TotaldNTPMixVol
SetPlateVolume FINISHED 0

(1-4-1)[ReadyTips]
AddToLogFile ">(1-4-1)"
Call GetNewTips	
Call PreAspiration2
GoHome

