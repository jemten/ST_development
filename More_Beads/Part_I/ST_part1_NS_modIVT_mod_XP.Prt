; ======================================================
; ======================================================
; ====================================================== Agencourt_PURIF
; The CA-BEAD_PURIF subroutine has been modified to suite Agencourt RNAClean XP (XP)
; Added a second capture for volumes just above 200 ul. 151005 AJ
; Added a split of bead and sample for even bigger volumes

(10-1-1)[AgencourtPurification]
AddToLogFile ">(10-1-1)"
; modifying the XP protocol to use tipwellvol for liquids and tipvol for air
; 150905 AJ

; String variables must be set before call:
; $cntXPStartingPos = Which plate (row) contains the sample
; $cntXPWorkingPos = Which plate (row) the beads and samples should incubate
; $cntXPElutionPos = Which plate (row) should be eluted in
; $cntXPBeadVol = amount of XP beads
; $cntXPElutionVol = Elution volume
; $cntPostXPTemp = Sets the temperature for next step when the elution starts
; $cntBufferPos = alternative position for the elution buffer 

AddToLogFile "Starting RNAClean purification protocol"
AddToLogFile "Starting position: " $cntXPStartingPos
AddToLogFile "Elution position: " $cntXPElutionPos

; Reset position
GoHome
Call HomeMotors
Org P W
ClearTipVolume
Gval cntXPElutionVol Add 1

(10-1-2)[Splitting volumes]
AddToLogFile ">(10-1-2)"
If $DoSplit EQ 1
	Gval SplitBeadsVol Let $cntXPBeadVol
	Gval SplitBeadsVol Div 2
	Gval GoBackDone Let 0
EndIf
	
(10-1-3)[GoXPBeads]
AddToLogFile "Starting bead stir up"
AddToLogFile ">(10-1-3)"
RunMessage "Stirring up beads"
LiqIn $xpAirVol #P_SPEED_H
MoveTo XP_BEADS
Call LowerTips

(10-2-1)[StirUpBeads]
AddToLogFile ">(10-2-1)"
Gval xpTemp1 Let @WellVolume
Gval xpTemp1 * 0.6
If $xpTemp1 > $xpBeadMixVol
    Gval xpTemp1 Let $xpBeadMixVol
EndIf

; Since tips weren't lowered enough when the remaining vol
; of beads is low:
RScanZ $scanDist #Z_SPEED_L
RMove Z -300

Do $xpStirUps
    WaitMsec $mixDelay1
    LiqInWell $xpTemp1 #P_SPEED_M
    WaitMsec $mixDelay2
    LiqOutWell $xpTemp1 #P_SPEED_MH
Loop
Down% 20 #Z_SPEED_H
LiqOut $xpAirVol #P_SPEED_H
WaitSec $1sec
ActionZ #Z_SPEED_H W
StandbyZ #Z_SPEED_LL W
LiqIn $xpAirVol #P_SPEED_H
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume

(10-2-2)[StirUpBeads2]
AddToLogFile ">(10-2-2)"
Gval TimeCheck Let @SECONDCOUNT
Gval TimeCheck - $startTime
AddToLogFile "This is starttime " $startTime
AddToLogFile "This is timecheck " $TimeCheck
AddToLogFile "This is secondcount " @SECONDCOUNT
If $TimeCheck GT 21600
	AddToLogFile "Doing second stir up"
	Call LowerTips
	Gval xpTemp1 Let @WellVolume
	Gval xpTemp1 * 0.6
	If $xpTemp1 > $xpBeadMixVol
		Gval xpTemp1 Let $xpBeadMixVol
	EndIf
	RScanZ $scanDist #Z_SPEED_L
	RMove Z -300

	Do $xpStirUps
		WaitMsec $mixDelay1
		LiqInWell $xpTemp1 #P_SPEED_M
		WaitMsec $mixDelay2
		LiqOutWell $xpTemp1 #P_SPEED_MH
	Loop
	Down% 20 #Z_SPEED_H
	LiqOut $xpAirVol #P_SPEED_H
	WaitSec $1sec
	ActionZ #Z_SPEED_H W
	StandbyZ #Z_SPEED_LL W
	LiqIn $xpAirVol #P_SPEED_H
EndIf
	
(10-3-1)[SetInitTemp]
AddToLogFile ">(10-3-1)"
_SetTemp 2 $startTemp

(10-3-2)[GetAllBeads]
AddToLogFile ">(10-3-2)"
AddToLogFile "Collecting beads"
RunMessage "Collecting beads"
MoveTo XP_BEADS
Call LowerTipsMore
LiqInWell $cntXPBeadVol #P_SPEED_L
WaitSec $1sec
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume

(10-3-3)[GoFragSep]
AddToLogFile ">(10-3-3)"
If $DoSplit EQ 1
	MoveTo XTRAWORKPOS
Else
	MoveTo $cntXPStartingPos
EndIf

(10-3-4)[CheckXPTemp]
AddToLogFile ">(10-3-4)"
RunMessage "Waiting for XP incubation temperature..."
_AlarmLevel $tempDif
;WAIT_LOOP
WaitSec  $1sec
_ChkAlarm 2
IfGoto <> 0 ;WAIT_LOOP
RunMessage "XP incubation temperature reached"

(10-3-5)[AddToSplit]
AddToLogFile ">(10-3-5)"
If $DoSplit EQ 1
	Down% 10 #Z_SPEED_H
	LiqOutWell $SplitBeadsVol #P_SPEED_M
	Down% 10 #Z_SPEED_H
	SideY% 90 #Y_SPEED_H
	SideY% -90 #Y_SPEED_H
	SideY% 0 #Y_SPEED_H
	MoveTo $cntXPStartingPos
EndIf

(10-3-6)[DispSample]
AddToLogFile ">(10-3-6)"
AddToLogFile "Dispensing beads to sample"
Call LowerTips
; #repl
LiqOutWell% 80 #P_SPEED_M
Down% 90 #Z_SPEED_H
LiqOutAll #P_SPEED_M
WaitMsec $500msec
LiqOut $xpAirVol #P_SPEED_H
Down% 10 #Z_SPEED_H
SideY% 90 #Y_SPEED_H
SideY% -90 #Y_SPEED_H
SideY% 0 #Y_SPEED_H
; Extra stuff to try and get out everything (101130):
StandbyZ #Z_SPEED_H
RMove Z 500 W
Org P
SideY% -90 #Y_SPEED_M
SideY% 90 #Y_SPEED_M
SideY% 0 #Y_SPEED_M
AMove Z 0 W
LiqIn $xpAirVol #P_SPEED_H
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume

(10-3-7)[TransferToWorkingPos]
AddToLogFile ">(10-3-7)"
AddToLogFile "Checking if transfer option is set"
If $cntNoTransfer EQ 1
	AddToLogFile "No transfer of samples"
	GoForward BindMix
EndIf
AddToLogFile "Transfering to Working position"
Gval cntAspVol Let @Wellvolume
Call AspirateVolume
MoveTo $cntXPWorkingPos
Call TipBlowOutAndTouchM
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume

(10-4-1)[BindMix]
;Extra mix for high volume
AddToLogFile ">(10-4-1)"
Call GeneralMix
GVal xpTemp1 Let @WellVolume
If $xpTemp1 GT 150
	Call PreAspiration
	Call GeneralMix
EndIf

(10-4-2)[EmptyTip]
AddToLogFile ">(10-4-2)"
Call ClearTipsXP

(10-4-3)[SplitMix]
AddToLogFile ">(10-4-3)"
If $DoSplit EQ 1
	MoveTo XTRAWORKPOS
	Call GeneralMix
	GVal xpTemp1 Let @WellVolume
	If $xpTemp1 GT 150
		Call PreAspiration
		Call GeneralMix
	EndIf
	Call ClearTipsXP
EndIf

(10-5-1)[LoopWhileIncubating]
AddToLogFile ">(10-5-1)"
AddToLogFile "Incubating..."
RunMessage "Incubating..."

Gval xpTemp1 Let $nMixes
Gval xpTemp1 Inc
Gval mixInterval Let $xpBeadIncTime
Gval mixInterval \ $xpTemp1
Gval mixCount Let 0
Gval xpTemp1 Let $mixInterval

StartWatch 1
;INC_LOOP
PassTime 1
If @Result < $xpBeadIncTime
    If $mixCount < $nMixes
        WaitTime 1 $xpTemp1
        MoveTo $cntXPWorkingPos
        Call LowerTips
        Call GeneralMix
        Call ClearTipsXP
		If $DoSplit EQ 1
			MoveTo XTRAWORKPOS
			Call LowerTips
			Call GeneralMix
			Call ClearTipsXP
		EndIf	
        Gval mixCount Inc
        ; Next mix should be in <mixInterval> seconds from now
        PassTime 1
        Gval xpTemp1 Let @Result
        Gval xpTemp1 + $mixInterval
    Else
        WaitTime 1 $xpBeadIncTime
    EndIf
    Goto ;INC_LOOP
EndIf

(10-6-1)[Capture_Beads]
AddToLogFile ">(10-6-1)"

(10-7-1)[Capture Part]
AddToLogFile ">(10-7-1)"
AddToLogFile "Staring bead capture"
RunMessage "Bead capture started"

(10-7-2)[GoFragSep]
AddToLogFile ">(10-7-2)"
If $GoBackDone EQ 0
	MoveTo XTRAWORKPOS
Else
	MoveTo $cntXPWorkingPos
EndIf

(10-7-3)[PreMixing]
AddToLogFile ">(10-7-3)"
Call LowerTips
Gval xpTemp1 Let @WellVolume
Gval xpTemp1 * 0.7
If $xpTemp1 > $xpBeadMixVol
    Gval xpTemp1 Let $xpBeadMixVol
EndIf
Do 10
  WaitMsec $mixDelay1
  LiqInWell $xpTemp1 #P_SPEED_MH
  WaitMsec $mixDelay2
  LiqOutWell $xpTemp1 #P_SPEED_MH
Loop

(10-7-4)[GetBeads1]
AddToLogFile ">(10-7-4)"
AddToLogFile "Get beads 1"
Gval xpTemp1 Let !@WellVolume(VolumeInWell)
If $xpTemp1 GT 200
	Gval xpTemp1 Div 2
	Gval TwoCaptures Let 1
EndIf
Call LowerTips
Gval xpTemp2 Let $xpTemp1
; xpTemp1 is 90% of the volume.
Gval xpTemp1 * 0.9
; xpTemp2 is 10% of the volume.
Gval xpTemp2 - $xpTemp1
Amove M $BF_MAG_HH
AddToLogFile "Aspirating 90% of well volume"
LiqInWell $xpTemp1 #P_SPEED_LL
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume
WaitMsec $500msec
ActionZ #Z_SPEED_H
RScanZ #DEL_SCAN_Z  #Z_SPEED_LL
DelMove Z #Z_DELUP_L W
AddToLogFile "Aspirating 10% of the volume"
LiqInWell $xpTemp2 #P_SPEED_LL
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume
Call LowerTips
AddToLogFile "Doing first mag_pass"

If $TwoCaptures EQ 1
	StandbyZ #Z_SPEED_M
	WaitMsec $500msec
EndIf

LiqIn #V_MAG_PASS #P_SPEED_LLL
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume
WaitMsec $500msec
LiqOut #V_MAG_PASS #P_SPEED_LLL

If $TwoCaptures EQ 1
	Call LowerTips
	WaitMsec $500msec
EndIf

LiqOutWell $xpTemp1 #P_SPEED_LLL
LiqOutWell $xpTemp2 #P_SPEED_LLL
AddToLogFile "TipVolume is: " @TipVolume "TipWellVolume is: " @TipWellVolume "WellVolume is: " @WellVolume

(10-7-5)[ReMix]
