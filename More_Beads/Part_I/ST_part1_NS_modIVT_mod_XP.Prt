;Gval maxMixVol - 10
;StandbyZ #Z_SPEED_H
;Do 10
;    LiqIn $maxMixVol #P_SPEED_H
;    WaitMsec $mixDelay
;    LiqOut $maxMixVol #P_SPEED_H
;    WaitMsec $mixDelay
;Loop
;WaitSec $1sec
;AMove Z 0
;Org P

; Washing complete
Call PreAspiration 
Return

